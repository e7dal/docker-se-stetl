<sml:SensorML
        xmlns:swe="http://www.opengis.net/swe/1.0.1"
        xmlns:sml="http://www.opengis.net/sensorML/1.0.1"
        xmlns:gml="http://www.opengis.net/gml"
        version="1.0.1">
    <sml:member>
        <sml:System>
            <sml:identification>
                <sml:IdentifierList>
                    <sml:identifier name="uniqueID">
                        <sml:Term definition="urn:ogc:def:identifier:OGC:1.0:uniqueID">
                            <sml:value>station-{station_id}</sml:value>
                        </sml:Term>
                    </sml:identifier>
                    <sml:identifier name="longName">
                        <sml:Term definition="urn:ogc:def:identifier:OGC:1.0:longName">
                            <sml:value>Smart Emission - Station {station_id}
                            </sml:value>
                        </sml:Term>
                    </sml:identifier>
                    <sml:identifier name="shortName">
                        <sml:Term definition="urn:ogc:def:identifier:OGC:1.0:shortName">
                            <sml:value>Smartem - {station_id}</sml:value>
                        </sml:Term>
                    </sml:identifier>
                </sml:IdentifierList>
            </sml:identification>
            <sml:capabilities name="offerings">
                <swe:SimpleDataRecord>
                    <swe:field name="offering-{station_id}">
                        <swe:Text definition="urn:ogc:def:identifier:OGC:offeringID">
                            <swe:value>offering-{station_id}</swe:value>
                        </swe:Text>
                    </swe:field>
                </swe:SimpleDataRecord>
            </sml:capabilities>
            <sml:capabilities name="featuresOfInterest">
                <swe:SimpleDataRecord>
                    <swe:field name="fid-{station_id}">
                        <swe:Text definition="featureOfInterestID">
                            <swe:value>fid-{station_id}</swe:value>
                        </swe:Text>
                    </swe:field>
                </swe:SimpleDataRecord>
            </sml:capabilities>
            <sml:position name="sensorPosition">
                <swe:Position referenceFrame="urn:ogc:def:crs:EPSG::4326">
                    <swe:location>
                        <swe:Vector gml:id="STATION_LOCATION-{station_id}">
                            <swe:coordinate name="easting">
                                <swe:Quantity axisID="x">
                                    <swe:uom code="degree"/>
                                    <swe:value>{station_lon}</swe:value>
                                </swe:Quantity>
                            </swe:coordinate>
                            <swe:coordinate name="northing">
                                <swe:Quantity axisID="y">
                                    <swe:uom code="degree"/>
                                    <swe:value>{station_lat}</swe:value>
                                </swe:Quantity>
                            </swe:coordinate>
                            <swe:coordinate name="altitude">
                                <swe:Quantity axisID="z">
                                    <swe:uom code="m"/>
                                    <swe:value>{station_altitude}</swe:value>
                                </swe:Quantity>
                            </swe:coordinate>
                        </swe:Vector>
                    </swe:location>
                </swe:Position>
            </sml:position>
            <sml:inputs>
                <sml:InputList>
                    <sml:input name="air">
                        <swe:ObservableProperty
                                definition="air"/>
                    </sml:input>
                </sml:InputList>
            </sml:inputs>
            <sml:outputs>
                <sml:OutputList>
                    <sml:output name="co">
                        <swe:Quantity definition="co">
                            <swe:uom code="ug/m3"/>
                        </swe:Quantity>
                    </sml:output>
                    <sml:output name="co2">
                        <swe:Quantity definition="co2">
                            <swe:uom code="ppm"/>
                        </swe:Quantity>
                    </sml:output>
                     <sml:output name="no2">
                        <swe:Quantity definition="no2">
                            <swe:uom code="ug/m3"/>
                        </swe:Quantity>
                    </sml:output>
                    <sml:output name="o3">
                        <swe:Quantity definition="o3">
                            <swe:uom code="ug/m3"/>
                        </swe:Quantity>
                    </sml:output>
                    <sml:output name="coraw">
                        <swe:Quantity definition="coraw">
                            <swe:uom code="kOhm"/>
                        </swe:Quantity>
                    </sml:output>
                     <sml:output name="no2raw">
                        <swe:Quantity definition="no2raw">
                            <swe:uom code="kOhm"/>
                        </swe:Quantity>
                    </sml:output>
                    <sml:output name="o3raw">
                        <swe:Quantity definition="o3raw">
                            <swe:uom code="kOhm"/>
                        </swe:Quantity>
                    </sml:output>
                     <sml:output name="pm10">
                        <swe:Quantity definition="pm10">
                            <swe:uom code="ug/m3"/>
                        </swe:Quantity>
                    </sml:output>
                    <sml:output name="pm2_5">
                        <swe:Quantity definition="pm2_5">
                            <swe:uom code="ug/m3"/>
                        </swe:Quantity>
                    </sml:output>
                    <sml:output name="noiseavg">
                        <swe:Quantity definition="noiseavg">
                            <swe:uom code="dB(A)"/>
                        </swe:Quantity>
                    </sml:output>
                    <sml:output name="noiselevelavg">
                         <swe:Quantity definition="noiselevelavg">
                             <swe:uom code="int"/>
                         </swe:Quantity>
                     </sml:output>
                    <sml:output name="temperature">
                         <swe:Quantity definition="temperature">
                             <swe:uom code="Celsius"/>
                         </swe:Quantity>
                     </sml:output>
                    <sml:output name="humidity">
                         <swe:Quantity definition="humidity">
                             <swe:uom code="Procent"/>
                         </swe:Quantity>
                     </sml:output>
                    <sml:output name="pressure">
                         <swe:Quantity definition="pressure">
                             <swe:uom code="HectoPascal"/>
                         </swe:Quantity>
                     </sml:output>
                </sml:OutputList>
            </sml:outputs>
        </sml:System>
    </sml:member>
</sml:SensorML>
